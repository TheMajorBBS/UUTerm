DECLARE SUB gm2 (selection!, sw!)
DECLARE SUB getchoice (choice!, sw!)
DECLARE SUB gm1 (selection!, sw!)
DIM songs$(1 TO 208)

'Over 300 songs and sound FX's

OPEN "i", 1, "musiclst"
FOR x = 1 TO 208
  LINE INPUT #1, line$
  songs$(x) = line$
NEXT x
CLOSE #1
SCREEN 0

start:
songnum = 0: sel = 0
CLS : COLOR 14, 0
LOCATE 4, 20: PRINT "Here are the catagories.. Choose one."
LOCATE 6, 1
FOR x = 1 TO 208
  IF LEFT$(songs$(x), 4) = "    " THEN
    sel = sel + 1
    COLOR 15, 0: PRINT SPACE$(19); sel;
    COLOR 11, 0: PRINT ") " + MID$(songs$(x), 5, 34)
  END IF
NEXT x

getchoice choice, 1
IF choice = 0 THEN COLOR 7, 0: CLS : END
chosethis = choice

CLS
COLOR 14, 0: sel = 0
LOCATE 4, 19: PRINT "Here are the Sounds.. Choose one."
LOCATE 6, 1
FOR x = 1 TO 208
  IF LEFT$(songs$(x), 4) = "    " THEN
    sel = sel + 1
    IF sel = choice THEN
      record = x + 1
      DO
        songnum = songnum + 1
        COLOR 15, 0: PRINT SPACE$(19); songnum;
        COLOR 11, 0: PRINT ") " + MID$(songs$(record), 6, 34)
        record = record + 1
        IF record = 209 THEN EXIT DO
        IF LEFT$(songs$(record), 4) = "    " THEN record = 209
      LOOP UNTIL record = 209
    END IF
END IF
NEXT x

doagain:
getchoice choice, 0
chose = ((chosethis - 1) * 15) + choice + chosethis
chose = chose + 1

COLOR 14, 0
LOCATE 22, 8: PRINT "Selection Now Playing: ";
COLOR 28, 0: PRINT MID$(songs$(chose), 6, 34)
selection = VAL(MID$(songs$(chose), 3, 3))
IF choice = 0 THEN GOTO start
IF selection < 196 THEN
  gm1 selection, sw
ELSE
  gm2 selection, sw
END IF
GOTO doagain

END

SUB getchoice (choice, sw)
IF sw = 1 THEN type$ = "category" ELSE type$ = "song"
COLOR 15, 0
again:
LOCATE 22, 8
PRINT "  Enter a "; type$; " or '0' to quit:"; SPACE$(30)
LOCATE 22, 42: INPUT " ", choice$
choice = VAL(choice$)
IF choice < 0 OR choice > 15 THEN GOTO again
END SUB

